[
  {
    "name": "input",
    "desc": "Input输入框",
    "component": "el-input",
    "@": [
      {
        "name": "input",
        "desc": "Input事件"
      }
    ],
    "props": {
      "label": {
        "type": "string",
        "default": "",
        "desc": "label值"
      },
      "placeholder": {
        "type": "string",
        "default": "",
        "desc": "占位说明"
      },
      "clearable": {
        "type": "bool",
        "default": true,
        "desc": "是否可清除"
      },
      "show-password": {
        "type": "bool",
        "default": true,
        "desc": "是否是密码框"
      },
      "size": {
        "type": "arrayOfType",
        "default": "",
        "desc": "指定大小",
        "list": [
          {
            "label": "迷你",
            "value": "mini"
          },
          {
            "label": "中",
            "value": "medium"
          },
          {
            "label": "小",
            "value": "small"
          }
        ]
      },
      "value": {
        "type": "string",
        "default": "",
        "desc": "最终的值"
      }
    }
  },
  {
    "name": "select",
    "desc": "Select选择框",
    "component": "el-select",
    "props": {
      "keyLabel": {
        "type": "string",
        "default": "",
        "desc": "option label"
      },
      "keyValue": {
        "type": "string",
        "default": "",
        "desc": "option value"
      },
      "value": {
        "type": "string",
        "default": "",
        "desc": "最终的值"
      },
      "multiple": {
        "type": "bool",
        "default": false,
        "desc": "是否多选"
      },
      "size": {
        "type": "arrayOfType",
        "default": "",
        "desc": "指定大小",
        "list": [
          {
            "label": "迷你",
            "value": "mini"
          },
          {
            "label": "中",
            "value": "medium"
          },
          {
            "label": "小",
            "value": "small"
          }
        ]
      },
      "clearable": {
        "type": "bool",
        "default": true,
        "desc": "是否可清除"
      },
      "placeholder": {
        "type": "string",
        "default": "",
        "desc": "占位说明"
      }
    }
  }
]
